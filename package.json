{
  "name": "Minimalistic-Calculator",
  "private": true,
  "description": "A simple calculator with a minimalist design",
  "version": "0.1.0",
  "author": "Sam Leyva",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "env-cmd -f .env.development gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",

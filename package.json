  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^3.0.0-alpha.0",
    "gatsby": "^2.21.9",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Montblac/minim-calculator"
  },
  "bugs": {
    "url": "https://github.com/Montblac/minim-calculator/issues"
  }
